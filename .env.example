# KotlinFFetch Environment Variables Example
# Copy this file to .env and configure your values

# HTTP Client Configuration
# HTTP_TIMEOUT_MS=30000
# HTTP_MAX_RETRIES=3

# Cache Configuration
# CACHE_MAX_SIZE_MB=100
# CACHE_DEFAULT_TTL_SECONDS=3600

# Development Settings
# DEBUG_MODE=false
# LOG_LEVEL=INFO

# Test Configuration
# TEST_BASE_URL=https://example.com
# TEST_API_KEY=your-test-api-key-here

# Publishing Configuration (for CI/CD)
# MAVEN_USERNAME=your-maven-username
# MAVEN_PASSWORD=your-maven-password
# GPG_SIGNING_KEY=your-gpg-key

# External Service URLs
# AEM_LIVE_BASE_URL=https://example.aem.live
# CONTENT_API_ENDPOINT=https://api.example.com

# Security Settings
# ALLOWED_HOSTS=example.com,trusted-domain.com
# ENABLE_HOSTNAME_VERIFICATION=true

# Performance Tuning
# COROUTINE_POOL_SIZE=10
# CONNECTION_POOL_SIZE=20